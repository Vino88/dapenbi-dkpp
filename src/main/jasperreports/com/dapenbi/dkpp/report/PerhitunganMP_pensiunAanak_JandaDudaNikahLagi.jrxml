	<queryString language="SQL">
		<![CDATA[SELECT 
    a.nip
    ,a.npwp as npwp
    ,a.nama_peserta
    ,a.nama_penerima_mp as nama_penerima_lama
    ,a.nama_penerima_mp_baru as nama_penerima_mp
    ,a.nik as nik
    ,a.nama_suskel_baru as suskel
    ,to_char(a.tgl_usia_25t_penerima_mp_baru, 'dd-mm-yyyy') as tgl_usia25
    ,to_char(a.tgl_meninggal_penerima_mp,'dd-mm-yyyy') as tgl_meninggal_penerima
    ,to_char(a.tgl_meninggal_peserta,'dd-mm-yyyy') as tgl_meninggal_peserta
    ,to_char(a.tgl_menikahlagi_penerima_mp, 'dd-mm-yyyy') as tgl_menikahlagi
    ,'K/1' as ptkp
    ,TO_CHAR(a.tgl_lahir_peserta, 'DD-MM-YYYY') AS tgl_lahir_peserta
    ,to_char(a.tgl_kerja_peserta, 'DD-MM-YYYY') as tgl_mulai_bekerja
    ,TO_CHAR(a.tgl_dptht_peserta, 'DD-MM-YYYY') as tgl_peserta
    ,a.no_sk_bi
    ,to_char(a.tgl_sk_bi,'dd-mm-yyyy') as tgl_sk_bi
    ,to_char(a.tgl_sk_dapenbi,'dd-mm-yyyy') as tgl_sk_dapenbi
    ,a.no_sp_bi
    ,a.no_sk_dapenbi
    ,to_char(a.tgl_sp_bi,'dd-mm-yyyy') as tgl_sp_bi
    ,TO_CHAR(a.tgl_henti_kerja_peserta,'DD-MM-YYYY') AS tgl_henti_kerja_peserta
    ,to_char(a.tgl_awal_bayar,'dd-mm-yyyy') as tgl_awal_bayar
    ,to_char(a.phdp_lama,'999,999,999,999.00') as gaji_pokok_terakhir
    ,a.phdp_baru
    ,to_char(a.mpyd_lama,'999,999,999,999.00') as mpyd_lama
    ,a.golongan_pangkat
    ,'Setingkat Asisten Direktur' as jabatan
    ,'KPwGI Yogyakarta -hardcoded' as satuan_kerja
     , to_char(a.tgl_mp_bulan_ke13 , 'DD-MM-YYYY') 
    , to_char(a.tgl_lahir_peserta , 'DD-MM-YYYY') || 
        '  -  ' || 
        TO_CHAR(a.tgl_henti_kerja_peserta, 'DD-MM-YYYY') || 
        ' = ' || 
        SUBSTR(a.usia_saat_henti, 1, 2) || 
        ' th ' ||  
        SUBSTR(a.usia_saat_henti, 3, 2) || 
        ' bl ' ||
        SUBSTR(a.usia_saat_henti, 5, 2) || 
        ' hr'
        as usia_saat_henti
    ,to_char(a.tgl_kerja_peserta,'DD-MM-YYYY') || 
        '  -  ' || 
        TO_CHAR(a.tgl_henti_kerja_peserta,'DD-MM-YYYY') || 
        ' = ' || 
        SUBSTR(a.masa_kerja_henti, 1, 2) || 
        ' th ' ||  
        SUBSTR(a.masa_kerja_henti, 3, 2) || 
        ' bl ' ||
        SUBSTR(a.masa_kerja_henti, 5, 2) || 
        ' hr'
        as masa_kerja_henti
    ,'2,5% x ' as penghasilan_pensiun_rumus
    ,SUBSTR(a.masa_kerja_mp, 1, 2) || ' ' || (CASE SUBSTR(a.masa_kerja_mp, 3, 2) WHEN '00' THEN '' ELSE SUBSTR(a.masa_kerja_mp, 3, 2) || '/12' END) as masa_kerja_mp
    ,TO_CHAR(a.phdp_lama,'999,999,999,999.00') AS phdp_lama
    ,to_char(a.mp_perbulan_rumus_lama,'999,999,999,999.00') AS mp_perbulan_rumus_lama
    ,'100%' as ik_berlaku_saat_tgl_bayar
    ,TO_CHAR(a.mps_20persen_lama,'999,999,999,999.00') AS mps_20persen_lama
    ,TO_CHAR(a.mp_perbulan_20persen_lama,'999,999,999,999.00') as mp_setelah_mps20_persen
    ,TO_CHAR(a.mp_perbulan_10juta_lama,'999,999,999,999.00') AS mp_perbulan_10juta_lama
    ,TO_CHAR(a.mp_perbulan_10juta_lama,'999,999,999,999.00') as mp_bulanan_setelah_mps_10jt
    ,TO_CHAR(a.mps_10juta_lama,'999,999,999,999.00') as mps_10juta_lama
    ,'12' as jumlah_bulan
    ,to_char(a.mp_bulan13,'999,999,999,999.00') as mp_bulan_ke13
    ,a.nilai_fs as NS
    ,a.nilai_fpr as fpr
    ,'20%' as rumus_mps_20_persen
    ,'80%' as rumus_80_persen
    ,a.kurang_bayar_mpyd as kurang_bayar
    ,nama_kantor_bayar as kantor_bayar
    ,to_char(a.tgl_mutasi,'dd Month YYYY' ,'nls_date_language = INDONESIAN') as sign_bulan_mutasi
    ,to_char(a.tgl_mutasi,'Month YEAR') as periode
    ,to_char(a.tgl_mutasi,'dd-mm-yyyy') as tgl_mutasi
    ,to_char(a.tgl_mutasi, 'Month' ,'nls_date_language = INDONESIAN') as bulan_mutasi
    ,to_char(a.tgl_mutasi, 'YYYY') as tahun_mutasi
    ,to_char(SYSDATE,'DD MONTH YYYY') AS SIGN_DATE
    ,TO_CHAR(a.tgl_meninggal_peserta, 'DD-MM-YYYY') AS tgl_meninggal_peserta
     ,to_char(a.tgl_mp_bulan_ke13, 'DD-MM-YYYY') as tgl_mp_bulan_ke13
     ,a.keterangan
     ,a.nilai_fs
     ,a.nilai_fpr
     ,to_char(to_date(a.periode_mutasi,'MMYYYY'),'Month YYYY','nls_date_language = INDONESIAN') as periode_mutasi
     ,(CASE WHEN a.id_kategori_penerima_mp_baru IN('2','20') AND a.jenis_kelamin_penerima_mp_baru = 'W' then 'PENSIUN JANDA' WHEN a.id_kategori_penerima_mp_baru IN('2','20') AND a.jenis_kelamin_penerima_mp_baru = 'P' then 'PENSIUN DUDA' WHEN a.id_kategori_penerima_mp_baru NOT IN ('1','2','11','20') then 'PENSIUN ANAK' WHEN a.id_kategori_penerima_mp_baru = '11' then 'AHLI WARIS / PIHAK YANG DITUNJUK' else 'PENSIUN HAPUS' END
     ) as jenis_pensiun
	 ,(CASE WHEN a.id_kategori_penerima_mp_baru IN('2','20') AND a.jenis_kelamin_penerima_mp_baru = 'P' then 'PENSIUN DUDA' 
            WHEN a.id_kategori_penerima_mp_baru IN('2','20') AND a.jenis_kelamin_penerima_mp_baru = 'W' then 'PENSIUN JANDA' 
            WHEN a.id_kategori_penerima_mp_baru NOT IN ('1','2','11','20') then 'PENSIUN ANAK' 
            WHEN a.id_kategori_penerima_mp_baru = '11' then 'AHLI WARIS / PIHAK YANG DITUNJUK' END) as jenis_manfaat_pensiun
	,(CASE WHEN a.id_kategori_penerima_mp IN('2','20') AND a.jenis_kelamin_penerima_mp = 'P' then 'PENSIUNAN DUDA MENIKAH LAGI' 
		WHEN a.id_kategori_penerima_mp IN('2','20') AND a.jenis_kelamin_penerima_mp = 'W' then 'PENSIUNAN JANDA MENIKAH LAGI' END
	) as penerima
	,(CASE WHEN a.id_kategori_penerima_mp IN('2','20') AND jenis_kelamin_penerima_mp = 'P' then 'Duda' WHEN a.id_kategori_penerima_mp IN('2','20') AND a.jenis_kelamin_penerima_mp = 'W' then 'Janda' else '' END
	) as pn_meninggal
	,to_char(add_months(TRUNC(to_date(a.tgl_menikahlagi_penerima_mp), 'MM'), 1),'DD-MM-YYYY') as tgl_mulai_dibayar
	,to_char(add_months(TRUNC(to_date(a.tgl_usia_25t_penerima_mp_baru), 'MM'), 1),'dd-mm-yyyy') as tgl_mp_anak_akhir
	,(SELECT to_char(cc.tgl_usia25,'dd-mm-yyyy') FROM tbl_m_keluarga_pensiun cc  where id_penerima_mp_baru = cc.id_keluarga) as tgl25
	,(SELECT to_char(add_months(TRUNC(to_date(cc.tgl_usia25), 'MM'), 1),'dd-mm-yyyy') FROM tbl_m_keluarga_pensiun cc  where id_penerima_mp_baru = cc.id_keluarga) as tgl_akhir 
	,(CASE WHEN a.tgl_mp_bulan_ke13 <= SYSDATE then '80%' else '100%' END) as kondisi_persen
	,(CASE WHEN a.tgl_mp_bulan_ke13 <= SYSDATE then to_char(a.mp_bulan13,'999,999,999,999.00') else to_char(a.mp_perbulan_rumus_lama,'999,999,999,999.00') END) as kondisi_mp
	FROM tbl_t_mutasi_pensiun a 
	WHERE a.periode_mutasi = $P{periode_mutasi} 
	AND a.id_kategori_mutasi = $P{id_kategori_mutasi}
	AND a.nip = $P{nip}]]>
	</queryString>
