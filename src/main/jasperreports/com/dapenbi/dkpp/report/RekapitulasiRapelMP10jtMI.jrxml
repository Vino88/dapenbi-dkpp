	<parameter name="periode_mutasi" class="java.lang.String"/>
	<parameter name="id_kantor_bayar" class="java.lang.String"/>
	<parameter name="tgl_henti" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[SELECT 
COUNT(a.nama_kantor_bayar) as jumlah_orang
,SUM(a.phdp_baru) as jml_phdp_baru
,SUM(a.phdp_lama) as phdp
,SUM(a.mpyd_lama) as jml_mpyd
,SUM(a.mps_10juta_lama) as mps_10jt
,SUM(a.mps_10juta_baru) as mp_setelah_mps10jt
,CASE SUM(a.kurang_bayar_mps10juta)
  WHEN 0 THEN SUM(a.lebih_bayar_mps10juta)
    ELSE SUM(a.kurang_bayar_mps10juta)
END AS rapel_mps10juta
,CASE ROUND(SUM(a.kurang_bayar_mps10juta),'-2')
  WHEN 0 THEN ROUND(SUM(a.lebih_bayar_mps10juta),'-2')
    ELSE ROUND(SUM(a.kurang_bayar_mps10juta),'-2')
END AS rapel_mps10juta_bulat
,a.id_kantor_bayar as kode_cabang
,a.nama_kantor_bayar as kantor_bayar
,to_char(a.tgl_henti_kerja_peserta,'dd-mm-yyyy') as tgl_berhenti
 ,to_char(a.tgl_mutasi, 'Month') as bulan_mutasi
 ,to_char(a.tgl_mutasi, 'YYYY') as tahun_mutasi
 ,to_char(a.tgl_mutasi,'dd Month yyyy','nls_date_language = INDONESIAN') as sign_date
  ,to_char(to_date(a.periode_mutasi,'MM YYYY','nls_date_language = INDONESIAN'),'Month YYYY','nls_date_language = INDONESIAN') as periode_mutasi
FROM tbl_t_mutasi_pensiun a WHERE periode_mutasi = $P{periode_mutasi} AND id_jenis_mps IN('2','4') AND (id_kantor_bayar LIKE '%$P!{id_kantor_bayar}%' OR  $P{id_kantor_bayar} = '00') AND  to_char(tgl_henti_kerja_peserta,'dd-mm-yyyy') = $P{tgl_henti} GROUP BY tgl_henti_kerja_peserta,nama_kantor_bayar, id_kantor_bayar, a.tgl_mutasi
,rapel_mpyd_kurang,rapel_mpyd_lebih, periode_mutasi]]>
	</queryString>
