	<queryString language="SQL">
		<![CDATA[SELECT 
phdp_lama 
,phdp_baru
,MPS_20PERSEN_LAMA
,mps_20persen_baru
,id_kantor_bayar
,id_golongan
,nama_kantor_bayar as kantor_bayar
,nip
,nama_peserta
,nama_penerima_mp 
,nilai_fs
,lebih_bayar_mps20persen
,kurang_bayar_mps20persen
,to_char(tgl_henti_kerja_peserta,'dd Month yyyy','nls_date_language = INDONESIAN') as tgl_henti_kerja_peserta

,CASE kurang_bayar_mps20persen
	WHEN 0  THEN lebih_bayar_mps20persen
    ELSE kurang_bayar_mps20persen
END AS rapel_mps20persen

,CASE kurang_bayar_mps20persen
	WHEN 0  THEN ROUND(lebih_bayar_mps20persen,'-2')
    ELSE ROUND (kurang_bayar_mps20persen,'-2')
END AS rapel_mps_20persen_pembulatan

,CASE kurang_bayar_mps20persen - ROUND (kurang_bayar_mps20persen,'-2')
WHEN 0 THEN lebih_bayar_mps20persen - ROUND(lebih_bayar_mps20persen,'-2')
ELSE kurang_bayar_mps20persen - ROUND (kurang_bayar_mps20persen,'-2') END AS pembulatan

 ,to_char(SYSDATE,'dd Month yyyy') as sign_date
 ,to_char(SYSDATE,'dd-mm-yyyy') as date_now
  ,to_char(to_date(periode_mutasi,'MM YYYY','nls_date_language = INDONESIAN'),'Month YYYY','nls_date_language = INDONESIAN') as periode_mutasi
FROM tbl_t_mutasi_pensiun WHERE periode_mutasi = $P{periode_mutasi} AND id_jenis_mps = '1' AND (id_kantor_bayar = $P{id_kantor_bayar} OR $P{id_kantor_bayar} = '00') AND to_char(tgl_henti_kerja_peserta,'dd-mm-yyyy') = $P{tgl_henti}
GROUP BY
nama_kantor_bayar 
,phdp_lama 
,phdp_baru
,mps_20persen_lama
,mps_20persen_baru
,id_kantor_bayar 
,id_golongan
,nip
,nama_peserta
,nama_penerima_mp 
,nilai_fs
,lebih_bayar_mps20persen
,kurang_bayar_mps20persen
,tgl_henti_kerja_peserta
,periode_mutasi]]>
	</queryString>
