	<queryString language="SQL">
		<![CDATA[select
    a.NAMA_KELUARGA
    ,b.NAMA_KATEGORI_PENERIMA
    ,to_char(a.tgl_lahir , 'DD-MM-YYYY' )  AS tgl_lahir
    ,TO_CHAR( a.tgl_menikah, 'DD-MM-YYYY' ) AS tgl_menikah
