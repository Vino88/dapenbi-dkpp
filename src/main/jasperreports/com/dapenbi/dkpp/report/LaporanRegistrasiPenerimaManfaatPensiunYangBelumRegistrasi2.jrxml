	<parameter name="tanggal_her" class="java.lang.String"/>
	<queryString>
		<![CDATA[select a.tahun_her  
    , a.tahun_her
    ,to_char(a.tgl_her,'dd-mm-yyyy') as tgl_her
    , to_char(a.tgl_her, 'dd Month yyyy','nls_date_language = INDONESIAN') as tgl_her_desc
    , a.nomor_her
    , a.nip
    , a.nama_peserta
    , a.nama_penerima
    , a.id_kantor_bayar
    , a.is_ada_kk
    , a.IS_ADA_KTP
    , a.IS_ADA_PHOTO
    , a.id_validasi
    , a.status_her_desc
    , b.nama_validasi
    ,to_char(SYSDATE,'dd Month yyyy') as sign_date
from TBL_T_HER_REGISTRASI_DETAIL a inner join TBL_R_STATUS_VALIDASI b on a.id_validasi=b.id_validasi
where a.status_her_desc = 'BELUM HEREGISTRASI' and to_char(a.tgl_her,'dd-mm-yyyy') = $P{tanggal_her}]]>
	</queryString>
