	<parameter name="periode_mutasi" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[select
	(select sum(d.pembulatan) from tbl_t_pembayaran_detail_mp d inner join tbl_t_pembayaran_header h on h.id_pembayaran = d.id_pembayaran where h.periode_pembayaran = to_char(add_months(TRUNC(to_date($P{periode_mutasi},'MMYYYY'),'MM'), -1),'MMYYYY')) as bln_lalu_bulat
	,(select sum(d.pembulatan) from tbl_t_pembayaran_detail_mp d inner join tbl_t_pembayaran_header h on h.id_pembayaran = d.id_pembayaran where h.periode_pembayaran = $P{periode_mutasi} AND id_status_bayar = '1' ) as bln_skrg_bulat
   ,(select sum(dmp.mp_bulanan)+SUM(dmp.kurang_bayar_mp_bulanan)+SUM(dmp.pembulatan) from tbl_t_pembayaran_detail_mp dmp inner join tbl_t_pembayaran_header h on h.id_pembayaran = dmp.id_pembayaran where h.periode_pembayaran = to_char(add_months(TRUNC(to_date($P{periode_mutasi},'MMYYYY'),'MM'), -1),'MMYYYY')) as mp_bulan_lalu
    ,(select COUNT(*) from tbl_t_pembayaran_detail_mp d inner join tbl_t_pembayaran_header h on h.id_pembayaran = d.id_pembayaran where h.periode_pembayaran = $P{periode_mutasi} AND id_status_bayar = '1') as jml_mutasi_bulan_skrg
    ,(select COUNT(*) from tbl_t_mutasi_pensiun a where id_kategori_mutasi = '01' and a.periode_mutasi = $P{periode_mutasi} AND id_status_bayar = '1') as jml_mutasi_pensiun_baru
    ,(select COUNT(*) from tbl_t_mutasi_pensiun a where id_kategori_mutasi IN('08','09','10') AND periode_mutasi = $P{periode_mutasi} AND id_status_bayar = '1') as jml_mutasi_pensiun_tunda
    ,(select COUNT(*) from tbl_t_mutasi_pensiun a where id_kategori_mutasi IN ('02') AND periode_mutasi = $P{periode_mutasi} AND id_status_bayar = '6') as jml_mutasi_meninggal_dunia
    ,(select COUNT(*) from tbl_t_mutasi_pensiun a where id_kategori_mutasi IN ('12') AND periode_mutasi = $P{periode_mutasi} AND id_status_bayar = '1') as jml_mutasi_mps100
    ,(select COUNT(*) from tbl_t_mutasi_pensiun a where id_kategori_mutasi IN('07','15') AND periode_mutasi = $P{periode_mutasi} AND id_status_bayar = '6') as jml_janda_duda_anak
    ,(select COUNT(*) from tbl_t_pembayaran_detail_mp d inner join tbl_t_pembayaran_header h on h.id_pembayaran = d.id_pembayaran where h.periode_pembayaran = to_char(add_months(TRUNC(to_date($P{periode_mutasi},'MMYYYY'),'MM'), -1),'MMYYYY')) as jml_penerima_bulan_lalu
    ,(select COUNT(*) from tbl_t_mutasi_pensiun a where id_kategori_pensiun = 'PT' AND a.periode_mutasi = to_char(add_months(TRUNC(to_date($P{periode_mutasi},'MMYYYY'),'MM'), -1),'MMYYYY')) as jml_pensiun_tunda_lalu
    ,(select COUNT(*) from tbl_t_mutasi_pensiun a where id_kategori_pensiun = 'PT' AND id_status_bayar = '1' AND a.periode_mutasi = $P{periode_mutasi}) as jml_pensiun_tunda_dibayar
   
     ,(select sum(dmp.mp_bulanan) from tbl_t_pembayaran_detail_mp dmp inner join tbl_t_pembayaran_header h on h.id_pembayaran = dmp.id_pembayaran where h.periode_pembayaran = $P{periode_mutasi}) as jml_mp_bln_skrg
    ,(select SUM(a.mpyd_lama)from tbl_t_mutasi_pensiun a where id_kategori_mutasi = '01' AND periode_mutasi = $P{periode_mutasi}) as jml_mpyd_pensiun_baru
    ,(select sum(a.mp_perbulan_rumus_lama) from tbl_t_mutasi_pensiun a where id_kategori_mutasi IN('08','09','10') AND periode_mutasi = $P{periode_mutasi}) as jml_mpyd_pensiun_tunda
    ,(select  sum(rapel_mpyd_kurang)- sum(nvl(kurang_bayar_mps20persen,0))- sum(nvl(kurang_bayar_mps10juta,0)) as kurang_bayar from  tbl_t_mutasi_pensiun where   id_kategori_mutasi  = '13' AND periode_mutasi = $P{periode_mutasi}) as jml_rapel_phdp
    ,(select sum(a.mpyd_baru) - sum(a.mpyd_lama) from tbl_t_mutasi_pensiun a where a.id_kategori_mutasi IN('13') AND periode_mutasi = $P{periode_mutasi}) as jml_mpyd_merit
    ,(select sum(a.mp_perbulan_rumus_lama) from tbl_t_mutasi_pensiun a where a.id_kategori_mutasi IN('02') AND periode_mutasi = $P{periode_mutasi} AND id_status_bayar = '6') as jml_mpyd_mutasi_hapus
    ,(select sum(a.mpyd_lama) from tbl_t_mutasi_pensiun a where a.id_kategori_mutasi IN('12') AND periode_mutasi = $P{periode_mutasi} AND a.id_validasi = '2' ) as jml_mpyd_mps100
    ,(select sum(a.mp_perbulan_rumus_lama) from tbl_t_mutasi_pensiun a where a.id_kategori_mutasi IN('07','15') AND periode_mutasi = $P{periode_mutasi} AND id_status_bayar = '6' AND id_validasi = '2') as jml_mpyd_janda_duda_anak
    ,(select sum(mp_perbulan_rumus_lama) - sum(a.mp_bulan13) from tbl_t_mutasi_pensiun a where id_kategori_mutasi = '11' AND periode_mutasi = $P{periode_mutasi} and id_status_bayar = '1' and id_validasi = '2') as jml_mpyd_mp_bulan13
    ,(select sum(a.lebih_bayar_mpyd) from tbl_t_mutasi_pensiun a where a.id_kategori_mutasi IN('21') AND periode_mutasi = $P{periode_mutasi}) as jml_kelebihan_mp_bulan_skrg
    ,(select sum(fp.mp_perbulan_rumus) from tbl_t_mutasi_perubahan_fpr_fs fp where fp.periode_mutasi = $P{periode_mutasi}) as jml_fpr_fs
    ,(select sum(up.MP_BULANAN_RAPEL) from tbl_t_mutasi_kenaikan_mp up where up.periode_mutasi = to_char(add_months(TRUNC(to_date($P{periode_mutasi},'MMYYYY'),'MM'), -1),'MMYYYY')) as jml_up_bln_lalu
     ,to_char(to_date(a.periode_mutasi,'MM YYYY','nls_date_language = INDONESIAN'),'Month YYYY','nls_date_language = INDONESIAN') as periode_mutasi
	,to_char(SYSDATE, 'dd Month yyyy','nls_date_language = INDONESIAN') as sign_date
	,(SELECT sum(k.kurang_bayar_mp) FROM tbl_t_kurang_bayar_detail k WHERE k.periode_mutasi = $P{periode_mutasi} AND k.id_alasan_kuranglebih_bayar = '4') as keterlambatan_data
	,(select sum(rapel_mpyd_kurang)- sum(nvl(kurang_bayar_mps20persen,0))- sum(nvl(kurang_bayar_mps10juta,0)) as kurang_bayar from tbl_t_mutasi_pensiun a where id_kategori_mutasi = '13' AND a.periode_mutasi = to_char(add_months(TRUNC(to_date($P{periode_mutasi},'MMYYYY'),'MM'), -1),'MMYYYY')) as jml_rapel_phdp_bln_lalu
from tbl_t_mutasi_pensiun a where periode_mutasi = $P{periode_mutasi}
GROUP BY a.periode_mutasi]]>
	</queryString>
